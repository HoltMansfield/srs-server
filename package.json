    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "config": "^1.21.0",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "mongo": "^0.1.0",
